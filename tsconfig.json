{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "esnext",
      "dom",
      "esnext.asynciterable"
    ],
    "importHelpers": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "typeRoots": [
      "node_modules/@types"
    ],
    "stripInternal": true,
    "skipLibCheck": true,
    "noLib": false,
    "declaration": false,
    "baseUrl": "./src",
    "outDir": "./dist",
    "types": ["node", "jest"],
    "sourceMap": false,
    "inlineSourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": false,
    "esModuleInterop": false
  },
  "compileOnSave": false,
  "suppressImplicitAnyIndexErrors": true,
  "exclude": [
    "node_modules",
    ".fusebox",
    ".build",
    "coverage",
    "public",
    "dist"
  ]
}