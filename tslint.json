{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "object-literal-sort-keys": false
    },
    "rules": {
        "no-angle-bracket-type-assertion": false,
        "object-literal-sort-keys": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore",
            "allow-pascal-case",
            "allow-snake-case"
        ]
    },
    "rulesDirectory": []
}