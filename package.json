{
  "name": "day_todo",
  "version": "0.0.1",
  "description": "Serverless Todo Application",
  "private": true,
  "repository": "github:pro_coders/makamekm/aws_todo",
  "keywords": [
    "now",
    "serverless",
    "react",
    "typescript",
    "jest",
    "tslint",
    "todo"
  ],
  "bin": {
    "collect-service": "./collectServerless.js"
  },
  "scripts": {
    "now-build": "run-s clean dist",
    "clean": "rm -rf public dist",
    "dist": "run-p dist:client dist:verify",
    "dist:client": "node ./fuse.js",
    "dist:verify": "tsc",
    "dev": "run-p dev:client dev:express",
    "dev:express": "ts-node-dev --respawn --transpileOnly ./dev.ts",
    "dev:client": "node ./fuse.dev.js",
    "test": "jest --forceExit --detectOpenHandles --collect-coverage"
  },
  "author": {
    "name": "Karpov Maxim",
    "email": "makame@mail.ru"
  },
  "license": "GNU",
  "devDependencies": {
    "@types/jest": "24.0.11",
    "fuse-box": "3.7.1",
    "jest": "24.4.0",
    "jest-runner-tslint": "1.0.5",
    "node-sass": "4.11.0",
    "nodemon": "1.18.10",
    "postcss": "7.0.14",
    "postcss-clean": "1.1.0",
    "react-test-renderer": "16.8.4",
    "react-testing-library": "^6.0.0",
    "reload": "2.4.0",
    "ts-jest": "24.0.0",
    "ts-node-dev": "1.0.0-pre.32",
    "tslint": "5.13.1"
  },
  "dependencies": {
    "@types/express": "4.16.1",
    "@types/react": "16.8.7",
    "@types/react-dom": "16.8.2",
    "antd": "^3.15.0",
    "apollo-server-express": "2.4.8",
    "body-parser": "1.18.3",
    "cookie-parser": "1.4.4",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "fs": "0.0.1-security",
    "graphql-hooks": "^3.3.3",
    "graphql-hooks-memcache": "^1.1.1",
    "graphql-hooks-ssr": "^1.0.7",
    "mime-types": "2.1.21",
    "mongodb": "3.2.2",
    "node-fetch": "^2.3.0",
    "npm-run-all": "4.1.5",
    "passport": "0.4.0",
    "passport-google-oauth": "2.0.0",
    "passport-local": "1.0.0",
    "prop-types": "15.7.2",
    "react": "16.8.4",
    "react-apollo": "^2.5.2",
    "react-apollo-hooks": "^0.4.3",
    "react-dom": "16.8.4",
    "react-helmet": "6.0.0-beta",
    "react-ioc": "^1.0.0",
    "react-router-config": "4.4.0-beta.6",
    "react-router-dom": "4.3.1",
    "rxjs": "^6.4.0",
    "rxjs-hooks": "^0.4.0",
    "serialize-javascript": "1.6.1",
    "serverless-http": "1.9.1",
    "styled-components": "4.1.3",
    "ts-loader": "5.3.3",
    "tslib": "1.9.3",
    "typegql": "0.6.1",
    "typeorm": "0.2.14",
    "typescript": "3.3.3333",
    "core-js": "3.0.0"
  },
  "jest": {
    "projects": [
      "./jest.tslint.json",
      "./jest.test.json"
    ],
    "collectCoverage": false,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/*.test.{ts,tsx}",
      "!**/*.d.{ts,tsx}"
    ]
  }
}
